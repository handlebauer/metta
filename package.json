        "dotenv": "^16.4.7",
        "eslint": "^9",
        "eslint-config-next": "15.1.5",
        "eslint-config-prettier": "^10.0.1",
        "husky": "^9.1.7",
        "playwright": "^1.49.1",
        "postcss": "^8",
        "prettier": "3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.10",
        "prompt-sync": "^4.2.0",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    }
}
