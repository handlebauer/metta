{
    "name": "metta",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "husky",
        "format": "prettier --write .",
        "supabase:gen-types": "bunx supabase gen types typescript --local > src/lib/supabase/types.ts",
        "supabase:init:local": "bunx supabase db reset && bun run supabase:gen-types && bun run supabase:seed:local",
        "supabase:seed:local": "bun run scripts/db/seed.ts",
        "supabase:init:prod": "NODE_ENV=production bunx supabase db reset --linked && bun run supabase:seed:prod",
        "supabase:seed:prod": "NODE_ENV=production bun run scripts/db/seed.ts",
        "shadcn:add": "bunx --bun shadcn@latest add",
        "test": "playwright test --config=playwright.config.ts",
        "test:smoke": "playwright test --config=playwright.config.ts --grep @smoke",
        "test:critical": "playwright test --config=playwright.config.ts --grep @critical",
        "email:test": "bun run scripts/email/test.ts",
        "verify-ticket": "bun run scripts/db/verify-ticket.ts",
        "test:firebreak": "bun run scripts/test-firebreak.ts"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.10.0",
        "@langchain/core": "^0.3.37",
        "@langchain/langgraph": "^0.2.43",
        "@langchain/openai": "^0.4.2",
        "@radix-ui/react-alert-dialog": "^1.1.4",
        "@radix-ui/react-avatar": "^1.1.2",
        "@radix-ui/react-checkbox": "^1.1.3",
        "@radix-ui/react-collapsible": "^1.1.2",
        "@radix-ui/react-dialog": "^1.1.5",
        "@radix-ui/react-dropdown-menu": "^2.1.4",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-popover": "^1.1.5",
        "@radix-ui/react-scroll-area": "^1.2.2",
        "@radix-ui/react-select": "^2.1.4",
        "@radix-ui/react-slot": "^1.1.1",
        "@radix-ui/react-switch": "^1.1.2",
        "@radix-ui/react-tabs": "^1.1.2",
        "@radix-ui/react-toast": "^1.2.5",
        "@react-email/render": "^1.0.4",
        "@sendgrid/mail": "^8.1.4",
        "@supabase/ssr": "^0.5.2",
        "@tabler/icons-react": "^3.28.1",
        "@tanstack/react-table": "^8.20.6",
        "@tiptap/pm": "^2.11.2",
        "@tiptap/react": "^2.11.2",
        "@tiptap/starter-kit": "^2.11.2",
        "ai": "^4.1.11",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dedent": "^1.5.3",
        "framer-motion": "^12.0.6",
        "lucide-react": "^0.473.0",
        "next": "15.1.5",
        "next-themes": "^0.4.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.54.2",
        "react-type-animation": "^3.2.0",
        "recharts": "^2.15.0",
        "sonner": "^1.7.2",
        "svix": "^1.45.1",
        "swr": "^2.3.0",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "typewriter-effect": "^2.21.0",
        "vaul": "^1.1.2",
